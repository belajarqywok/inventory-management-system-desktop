	<parameter name="invoicePurchasing" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT steel_shop.purchasing.purchase_date,
	steel_shop.purchasing.invoice_number,
	steel_shop.purchasing.stock_id,
	steel_shop.purchasing.brand,
	steel_shop.purchasing.product_type,
	steel_shop.purchasing.price,
	steel_shop.purchasing.sub_total,
	steel_shop.purchasing.price_total,
	steel_shop.purchasing.supplier_id,
	steel_shop.purchasing.supplier_name,
	steel_shop.purchasing.quantity,
	steel_shop.suppliers.supplier_id,
	steel_shop.suppliers.supplier_name,
	steel_shop.stocks.stock_id,
	steel_shop.stocks.category_id,
	steel_shop.stocks.purchase_price,
	steel_shop.categories.category_id,
	steel_shop.categories.brand,
	steel_shop.categories.product_type
FROM steel_shop.purchasing
	INNER JOIN steel_shop.suppliers ON 
	 steel_shop.purchasing.supplier_id = steel_shop.suppliers.supplier_id 
	INNER JOIN steel_shop.stocks ON 
	 steel_shop.purchasing.stock_id = steel_shop.stocks.stock_id 
	INNER JOIN steel_shop.categories ON 
	 steel_shop.stocks.category_id = steel_shop.categories.category_id
	 WHERE steel_shop.purchasing.invoice_number LIKE  $P{invoicePurchasing}]]>
	</queryString>
