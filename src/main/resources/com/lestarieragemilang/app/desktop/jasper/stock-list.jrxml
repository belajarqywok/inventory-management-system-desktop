	<queryString language="SQL">
		<![CDATA[SELECT steel_shop.stocks.stock_id,
	steel_shop.stocks.category_id,
	steel_shop.stocks.quantity,
	steel_shop.stocks.purchase_price,
	steel_shop.stocks.selling_price,
	steel_shop.categories.category_id,
	steel_shop.categories.brand,
	steel_shop.categories.product_type,
	steel_shop.categories.size,
	steel_shop.categories.weight,
	steel_shop.categories.weight_unit
FROM steel_shop.stocks
	INNER JOIN steel_shop.categories ON 
	 steel_shop.stocks.category_id = steel_shop.categories.category_id 
WHERE 
	 steel_shop.categories.brand LIKE $P{brandStock} 
	 OR steel_shop.categories.product_type LIKE $P{typeStock} 
	 OR steel_shop.categories.size LIKE $P{sizeStock} 
	 OR steel_shop.categories.weight LIKE $P{weightStock} 
	 OR steel_shop.categories.weight_unit LIKE $P{unitStock} 
	 OR steel_shop.stocks.quantity LIKE $P{stock} 
	 OR steel_shop.stocks.purchase_price LIKE $P{purchasePriceStock} 
	 OR steel_shop.stocks.selling_price LIKE $P{sellingPriceStock}]]>
	</queryString>
