	<parameter name="invoiceSales" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT steel_shop.sales.sale_date,
	steel_shop.sales.invoice_number,
	steel_shop.sales.stock_id,
	steel_shop.sales.brand,
	steel_shop.sales.product_type,
	steel_shop.sales.price,
	steel_shop.sales.sub_total,
	steel_shop.sales.price_total,
	steel_shop.sales.customer_id,
	steel_shop.sales.customer_name,
	steel_shop.sales.quantity,
	steel_shop.stocks.stock_id,
	steel_shop.stocks.category_id,
	steel_shop.stocks.selling_price,
	steel_shop.categories.category_id,
	steel_shop.categories.brand,
	steel_shop.categories.product_type,
	steel_shop.customers.customer_id,
	steel_shop.customers.customer_name
FROM steel_shop.sales
	INNER JOIN steel_shop.stocks ON 
	 steel_shop.sales.stock_id = steel_shop.stocks.stock_id 
	INNER JOIN steel_shop.customers ON 
	 steel_shop.sales.customer_id = steel_shop.customers.customer_id 
	INNER JOIN steel_shop.categories ON 
	 steel_shop.stocks.category_id = steel_shop.categories.category_id
	 WHERE steel_shop.sales.invoice_number LIKE $P{invoiceSales} ]]>
	</queryString>
